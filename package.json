{
  "name": "user-office-frontend",
  "version": "1.1.0",
  "description": "User office frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-frontend.git"
  },
  "private": true,
  "dependencies": {
    "@esss-swap/duo-localisation": "^1.1.15",
    "@esss-swap/duo-validation": "^1.1.44",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-graphql-request": "^1.17.7",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@material-ui/core": "~4.11.2",
    "@material-ui/icons": "~4.11.2",
    "@tinymce/tinymce-react": "^3.8.4",
    "@types/dateformat": "~3.0.1",
    "@types/jsonwebtoken": "~8.5.0",
    "@types/react": "^16.14.2",
    "@types/react-beautiful-dnd": "~11.0.5",
    "@types/react-dom": "~16.9.10",
    "@types/react-router": "~5.1.10",
    "@types/react-router-dom": "~4.3.5",
    "@types/tinymce": "~4.5.24",
    "@types/yup": "~0.29.11",
    "clsx": "~1.1.1",
    "date-fns": "~2.16.1",
    "dateformat": "~3.0.3",
    "express": "~4.17.1",
    "express-favicon": "~2.0.1",
    "express-http-proxy": "~1.6.0",
    "formik": "^2.2.6",
    "formik-material-ui": "^3.0.1",
    "formik-material-ui-pickers": "0.0.12",
    "graphql": "~14.7.0",
    "graphql-request": "~1.8.2",
    "html-react-parser": "~0.14.3",
    "http-proxy-middleware": "~0.21.0",
    "immer": "~8.0.1",
    "jsonwebtoken": "~8.5.1",
    "material-table": "~1.62.0",
    "notistack": "~0.9.17",
    "path": "~0.12.7",
    "query-string": "^6.13.8",
    "react": "~16.14.0",
    "react-beautiful-dnd": "~13.0.0",
    "react-cookie": "~4.0.3",
    "react-dom": "~16.14.0",
    "react-router": "~5.2.0",
    "react-router-dom": "~5.2.0",
    "react-scripts": "^3.4.4",
    "tinymce": "^5.6.2",
    "typescript": "~3.9.6",
    "use-query-params": "^1.1.9",
    "yup": "~0.29.3"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "dev": "react-scripts start",
    "dev:docker": "npm install && react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "debug": "react-scripts --inspect-brk start",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix:bash": "eslint --fix $(git diff --name-only --diff-filter=ACMRTUXB develop | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "eject": "react-scripts eject",
    "start": "node server.js",
    "cypress:open": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress open --config defaultCommandTimeout=15000",
    "cy:run": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress run --config defaultCommandTimeout=15000",
    "generate:local": "sed \"s@<SCHEMA_URL>@http://localhost:4000/graphql@g\" codegen_template.yml > codegen_tmp.yml && graphql-codegen -c codegen_tmp.yml && rm ./codegen_tmp.yml",
    "generate:local:windows": "powershell \"$c = Get-Content codegen_template.yml; $c -replace '<SCHEMA_URL>', 'http://localhost:4000/graphql' | Set-Content codegen_tmp.yml; graphql-codegen -c codegen_tmp.yml; Remove-Item codegen_tmp.yml\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "~6.8.1",
    "@types/faker": "~4.1.12",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "cypress": "6.1.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-cypress": "~2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "~2.5.1",
    "faker": "~4.1.0",
    "husky": "~4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "~1.19.1",
    "react-test-renderer": "~16.13.1",
    "source-map-explorer": "^2.5.0"
  }
}
