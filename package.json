{
  "name": "user-office-frontend",
  "version": "1.1.0",
  "description": "User office frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-frontend.git"
  },
  "private": true,
  "dependencies": {
    "@date-io/luxon": "^2.13.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.0",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@material-table/core": "^0.2.26",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@mui/styles": "^5.5.0",
    "@tinymce/tinymce-react": "^3.13.1",
    "@types/react": "^17.0.39",
    "@types/mime": "^2.0.3",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.13",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/tinymce": "^4.6.5",
    "@types/yup": "^0.29.13",
    "@user-office-software/duo-localisation": "^1.2.0",
    "@user-office-software/duo-validation": "^3.1.5",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "formik-mui-lab": "^1.0.0-alpha.3",
    "graphql": "^15.8.0",
    "graphql-request": "^3.7.0",
    "html-react-parser": "^1.4.8",
    "immer": "^9.0.7",
    "jwt-decode": "^3.1.2",
    "luxon": "^2.3.1",
    "mime": "^3.0.0",
    "notistack": "^2.0.3",
    "query-string": "^6.14.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "tinymce": "^5.10.3",
    "typescript": "^4.6.2",
    "use-query-params": "^1.2.3",
    "yup": "^0.32.11"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "dev": "react-scripts start",
    "debug": "react-scripts --inspect-brk start",
    "dev:docker": "npm install && react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix:bash": "eslint --fix $(git diff --name-only --diff-filter=ACMRTUXB develop | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "eject": "react-scripts eject",
    "start": "npm run dev",
    "cy:open": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress open --config defaultCommandTimeout=15000",
    "cy:run": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress run --config defaultCommandTimeout=15000",
    "generate:local": "node scripts/generate-graphql-sdk.js",
    "generate:local:windows": "powershell \"node scripts/generate-graphql-sdk.js\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./node_modules/eslint/bin/eslint.js --fix $(git diff --name-only --diff-filter=ACMRTUXB develop | grep -E '(.js$|.ts$|.tsx$)')"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.2.1",
    "@types/faker": "^5.5.9",
    "@types/luxon": "^2.3.0",
    "@types/node": "^16.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cypress": "^9.5.0",
    "cypress-file-upload": "^6.0.0-beta.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "faker": "5.5.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.6.1",
    "react-test-renderer": "^17.0.0",
    "source-map-explorer": "^2.5.2"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}