  },
  "proxy": "http://localhost:4100",
  "scripts": {
    "dev": "react-scripts start",
    "dev:docker": "npm install && react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix:bash": "eslint --fix $(git diff --name-only --diff-filter=ACMRTUXB develop | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "eject": "react-scripts eject",
    "start": "node server.js",
    "cy:open": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress open --config defaultCommandTimeout=15000",
    "cy:run": "env CYPRESS_SVC_ACC_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..y_coY649frw5dgl549tGjirF99nqwz1-BrUAILhE6pI cypress run --config defaultCommandTimeout=15000",
    "generate:local": "node scripts/generate-graphql-sdk.js",
    "generate:local:windows": "powershell \"node scripts/generate-graphql-sdk.js\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/faker": "^5.1.7",
    "@types/jest": "^24.0.16",
    "@types/node": "12.6.9",
    "@types/react-test-renderer": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cypress": "^8.0.0",
    "cypress-file-upload": "^3.5.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "faker": "^5.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.0",
    "source-map-explorer": "^2.5.2"
  }
}
